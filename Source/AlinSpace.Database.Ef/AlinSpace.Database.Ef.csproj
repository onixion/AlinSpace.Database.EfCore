  <ItemGroup>
    <ProjectReference Include="..\AlinSpace.Database\AlinSpace.Database.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

